.con{
  text-align: center;
}
.con>image{
  width:220rpx;height:205rpx;
  margin-top:150rpx;
}
.row{
  display:flex;justify-content:space-between;
  margin:50rpx
}
.row_one{
  display:flex;
}
.row_one>image{
  width:120rpx;height:120rpx;border-radius: 50%;
}
.row_one>.avator{
  font-size:120rpx;
  color:rgb(48, 47, 47);
}
.row_one>.name{
  line-height: 120rpx;
  margin-left: 20rpx;
}
.row_two{
  display:flex;
  line-height: 60rpx;
  background:linear-gradient(to right,#E4F9E7,#B4E7BE);
  color:#069842;
  height:60rpx;
  border-radius: 40rpx;
  padding:0 35rpx;
  margin-top:32rpx;
}
.row_two>text{
  color:#069842;
  font-size:30rpx;
  margin-left:10rpx
}
.box{
  display:flex;
  justify-content: space-between;
  margin: 0 50rpx;
  padding: 30rpx;
  background: #EBF6F0;
  box-shadow: 1px 3px 0px 1px #DCEBE3;
  border-radius: 15rpx;
}
.boxc>view{
  font-size:60rpx;font-weight:bold;
  margin-bottom:20rpx;
}
.boxc>text{
  color:#818985;
  font-size:30rpx
}
.text{
  margin-top:50rpx;font-size:38rpx;font-weight:bold;
  color:rgb(41, 40, 40);
}
.audit{
  text-align: left;
  margin:50rpx;
  font-size:30rpx;
  margin-bottom:80rpx;
  line-height: 60rpx;
  margin-top:100rpx;
}
.btn{
  background: rgb(8,151,69);
  color: #fff;
  text-align: center;
  padding: 25rpx 60rpx;
  width: 300rpx;
  margin: 0 auto;
  border-radius: 10rpx;
}
.btn1{
  background: rgb(8,151,69);
  color: #fff;
  text-align: center;
  padding: 25rpx 0;
  width: 650rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-bottom:20rpx
}
.btn2{
  background: #fff;
  color: rgb(8,151,69);
  border:1px solid rgb(8,151,69);
  text-align: center;
  padding: 25rpx 0;
  width: 650rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-bottom:20rpx
}